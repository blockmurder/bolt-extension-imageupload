{
	"name" : "blockmurder/imageupload",
	"description" : "This extension allows you to create galleries from a specific folder in /files. It uses the title of the entry as folder",
	"type" : "bolt-extension",
	"keywords" : [
		"Images",
		"Pictures",
		"Upload",
		"Watermark"
	],
	"require" : {
		"bolt/bolt" : ">=2.0.0,<3.0.0"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "blockmurder",
			"email" : "info@blockmurder.ch"
		}
	],
	"autoload" : {
		"files" : [
			"init.php"
		],
		"psr-4" : {
			"Bolt\\Extension\\blockmurder\\ImageUpload\\" : [
				"",
				"server/",
				"assets/"
			]
		}
	},
	"extra" : {
		"bolt-assets" : "assets/",
		"branch-alias" : {
			"dev-master" : "dev-develop"
		}
		"bolt-screenshots" : [
			"screenshots/ready_for_upload.png",
			"screenshots/images_uploaded.png",
			"screenshots/display_uploaded_image.png",
			"screenshots/unsorted_menu.png"
		]
	}
}
