{
	"name" : "blockmurder/imageupload",
	"description" : "This extension allows you to create galleries from a specific folder in /files. It uses the title of the entry as folder",
	"type" : "bolt-extension",
	"keywords" : [
		"Images",
		"Pictures",
		"Upload",
		"Watermark"
	],
	"require" : {
		"bolt/bolt" : ">=2.0.0,<3.0.0"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "blockmurder",
			"email" : "info@blockmurder.ch"
		}
	],
	"autoload" : {
		"files" : [
			"init.php"
		],
		"psr-4" : {
			"Bolt\\Extension\\blockmurder\\ImageUpload\\" : [
				"",
				"server/"
			]
		}
	},
	"extra" : {
		"bolt-assets" : "assets/"
	}
}